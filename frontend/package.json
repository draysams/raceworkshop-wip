    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "tsc-watch": "^7.1.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
